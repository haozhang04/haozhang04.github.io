<mujoco model="brook">
  <compiler angle="radian" meshdir="flash/meshes_xml" autolimits="true"/>
  <option cone="elliptic" impratio="100"/>


  <default>
    <default class="brook">
      <!-- 几何体默认属性：摩擦、边缘间隙、接触条件维度 -->
      <geom friction="0.6" margin="0.001" condim="1"/>
      <!-- 关节默认属性：阻尼、电枢、摩擦损失 -->
      <joint damping="2" armature="0.01" frictionloss="0.2"/>
      <!-- 位置控制默认属性：比例增益、力范围 -->
      <position kp="100"/>

      <!-- motor_pos -->
      <!-- 髋关节位置属性 -->
      <default class="hip">
        <joint range="-1.0472 1.0472"/>
        <position ctrllimited="true" ctrlrange="-1.0472 1.0472"/>
      </default>
      <!-- 大腿位置属性 -->
      <default class="thigh">
        <joint range="-0.54 4.55"/>
        <position ctrllimited="true" ctrlrange="-0.54 4.55"/>
      </default>
      <!-- 小腿位置属性 -->
      <default class="calf">
        <joint range="-2.7227 -0.83776"/>
        <position ctrllimited="true" ctrlrange="-2.7227 -0.83776"/>
      </default>

      <!-- motor_velocity -->
      <default class="hip_thigh_velocity">
        <velocity ctrllimited="true" ctrlrange="-30.1 30.1"/>
      </default>
      <default class="calf_velocity">
        <velocity ctrllimited="true" ctrlrange="-20.07 20.07"/>
      </default>

      <!-- motor_tau -->
      <default class="hip_thigh_tau">
        <motor gear="1" ctrllimited="true" ctrlrange="-23.7 23.7"/>
      </default>
      <default class="calf_tau">
        <motor gear="1" ctrllimited="true" ctrlrange="-36 36"/>
      </default>

      <!-- 足端摩擦参数 -->
      <default class="foot_parameter">
        <geom rgba="1 1 1 0" priority="1" solimp="0.015 1 0.031" condim="6" friction="0.8 0.02 0.01"/>
      </default>

    </default>
  </default>
  

  <asset>
    <material name="matte_black"       rgba="0.157 0.157 0.157 1"/>
